      <umbNewsItem id="1068" key="e7f0bc60-038d-447b-a13b-07d24e751ace" parentID="1065" level="3" creatorID="0" sortOrder="2" createDate="2015-12-06T16:21:21" updateDate="2017-05-23T22:18:26" nodeName="The future of Grid Layouts" urlName="the-future-of-grid-layouts" path="-1,1060,1065,1068" isDoc="" nodeType="1057" creatorName="Admin" writerName="Admin" writerID="0" template="1052" nodeTypeAlias="umbNewsItem">
